<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.0</TargetFramework>
    <Version>1.0.5</Version>
    <Authors>Sir Phillip Tubell</Authors>
    <Company />
    <Description>A Functional / Monadic library for .Net</Description>
    <Copyright>2017</Copyright>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
    <PackageLicenseUrl>https://github.com/sirphilliptubell/DotNetFunctional/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/sirphilliptubell/DotNetFunctional</PackageProjectUrl>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile>bin\Release\netstandard1.0\DotNetFunctional.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.ValueTuple" Version="4.3.1" />
  </ItemGroup>

</Project>